(function cookiebarScript($, Drupal, drupalSettings) {
  'use strict';

  Drupal.behaviors.checkCountry = {
    attach: function (context) {
      var eu_country_codes = [];
      var client_country;
      var cookiebar_active = false;

      if (drupalSettings.pin_blocks_eu_cookie_bar) {
        if (drupalSettings.pin_blocks_eu_cookie_bar.eu_countries) {
          eu_country_codes =
            drupalSettings.pin_blocks_eu_cookie_bar.eu_countries;
        }
        if (drupalSettings.pin_blocks_eu_cookie_bar.active) {
          cookiebar_active = drupalSettings.pin_blocks_eu_cookie_bar.active;
        }
      }
      if ('cookie' in $) {
        client_country = $.cookie('_ss_country_code');
      }
      // If our client_country is in the EU, remove the disabling.
      if (
        cookiebar_active &&
        client_country &&
        eu_country_codes.indexOf(client_country.toUpperCase()) !== -1
      ) {
        $('.alerts--eu-cookie-bar', context).removeClass('alerts--disabled');
      }
    }
  };
})(jQuery, Drupal, drupalSettings);
